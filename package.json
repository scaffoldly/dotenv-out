{
  "name": "dotenv-out",
  "version": "1.0.2",
  "description": "This CLI is a wrapper for dotenv and will output a resultant file with the compiled environment variables in a particular language format.",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && cp -R templates dist/",
    "prepare": "yarn build",
    "start": "ts-node index.ts"
  },
  "bin": {
    "dotenv-out": "./dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "repository": "git@github.com:scaffoldly/dotenv-out",
  "author": "Christian Nuss",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "ejs": "^3.1.6",
    "yargs": "^17.0.1"
  },
  "keywords": [
    "cli",
    "scaffoldly",
    "dotenv",
    "dotenv-cli",
    "output",
    "typescript",
    "generator"
  ],
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/node": "^15.12.5",
    "@types/yargs": "^17.0.0",
    "@vercel/ncc": "^0.28.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}