{
  "name": "dotenv-out",
  "version": "1.0.2-1",
  "description": "This CLI is a wrapper for dotenv and will output a resultant file with the compiled environment variables in a particular language format.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "ncc build index.js && cp -R templates dist/",
    "prepare": "yarn build"
  },
  "bin": {
    "dotenv-out": "./dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "repository": "git@github.com:scaffoldly/dotenv-out",
  "author": "Christian Nuss",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "ejs": "^3.1.6",
    "yargs": "^17.0.1"
  },
  "keywords": [
    "cli",
    "scaffoldly",
    "dotenv",
    "dotenv-cli",
    "output",
    "typescript",
    "generator"
  ],
  "devDependencies": {
    "@vercel/ncc": "^0.27.0"
  }
}